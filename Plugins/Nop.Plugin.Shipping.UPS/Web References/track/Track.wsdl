      <xsd:import schemaLocation="Error1.1.xsd" namespace="http://www.ups.com/XMLSchema/XOLTWS/Error/v1.1" />
      <xsd:import schemaLocation="TrackWebServiceSchema.xsd" namespace="http://www.ups.com/XMLSchema/XOLTWS/Track/v2.0" />
